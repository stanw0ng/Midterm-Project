<%- include('partials/_header', {title: "Read Stories - Open Book"}); %>

<div class = 'container'>
  <section class = 'all-stories half'>
    <h2>All Stories</h2>
    <ul>
      <% allStories.forEach(story => { %>
        <li><a href="/read/<%= story.id %>"><%= story.story_title %></a></li>
        <p>Description<br>Lorem, ipsum dolor sit amet consectetur adipisicing elit. Sunt nisi rerum sint vitae error enim adipisci optio voluptatum inventore, fugit velit asperiores a non iure cum nostrum. Ipsam, atque debitis.</p>
      <% }); %>
    </ul>
  </section>
  <section class = 'my-stories half'>
    <h2>My Stories</h2>
    <ul>
      <% myStories.forEach(story => { %>
        <li><a href="/read/<%= story.id %>"><%= story.story_title %></a></li>
      <% }); %>
    </ul>
  </section>
</div>

<%- include('partials/_footer'); %>
